selectOne=SELECT * FROM EMPLOYEE WHERE EMP_ID = ?
selectList=SELECT * FROM EMPLOYEE
selectMap=SELECT * FROM EMPLOYEE
selectSearchName=SELECT * FROM EMPLOYEE WHERE EMP_NAME LIKE ?
selectSearchHireDate=SELECT * FROM EMPLOYEE WHERE HIRE_DATE = ?
selectSearchSalary=SELECT * FROM EMPLOYEE WHERE SALARY >= ? AND SALARY<= ?
insertEmployee=INSERT INTO EMPLOYEE VALUES((SELECT MAX(EMP_ID)+1 FROM EMPLOYEE),?,?,?,?,SYSDATE, (SELECT JOB_ID FROM JOB WHERE JOB_TITLE LIKE ?),?,?,?,(SELECT EMP_ID FROM EMPLOYEE WHERE EMP_NAME NOT LIKE ? AND EMP_NAME LIKE ?),(SELECT DEPT_ID FROM DEPARTMENT WHERE DEPT_NAME LIKE ?))
deleteEmployee=DELETE FROM EMPLOYEE WHERE EMP_ID=?
updateEmployee=UPDATE EMPLOYEE SET EMAIL =?,PHONE = ?, MARRIAGE = ? WHERE EMP_ID = ?